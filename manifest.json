{
    "manifest_version": 2,
    "name": "YouTube Redirector",
    "version": "1.0",
    "description": "Redirects www.youtube.com to music.youtube.com",
    "permissions": ["webNavigation"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  